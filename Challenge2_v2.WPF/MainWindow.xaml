<Window x:Class="Challenge2_v2.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Challenge2_v2.WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Closed="Window_Closed">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="200"/>
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <Grid Grid.Column="0" Grid.Row="0">
            <Grid.RowDefinitions>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
            </Grid.RowDefinitions>
            <StackPanel Grid.Row="0">
                <Button Margin="20" x:Name="btnProcedures" Height="50" Click="btnProcedures_Click">Procedures</Button>
                <Button Margin="20" x:Name="btnOwners" Height="50" Click="btnOwners_Click">Owners</Button>
            </StackPanel>
        </Grid>
        <Frame x:Name="frmMain" Grid.Column="1" NavigationUIVisibility="Hidden"></Frame>
    </Grid>
</Window>
